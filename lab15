#include <stdio.h>

const int maxn = 8;

int main() {
    int m[maxn][maxn], n;
    scanf("%d", &n);
    for (int i = 0; i != n; ++i) {
        for (int j = 0; j != n; ++j) {
            scanf("%d", &m[i][j]);
        }
    }
    int i_max = 0, j_max = 0, i_min = 0, j_min = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (m[i][j] < m[i_min][j_min]) {
                i_min = i;
                j_min = j;
            }
            if (m[i][j] > m[i_max][j_max]) {
                i_max = i;
                j_max = j;
            }
        }
    }
    int res[maxn][maxn];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            res[i][j] = m[i][j_min] * m[i_max][j];
        }
    }
    printf("The result:\n",j_min + 1,i_max + 1);
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d ",res[i][j]);
        }
        printf("\n");
    }
    return 0;
}
