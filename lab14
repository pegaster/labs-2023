#include <stdio.h>
#include <stdlib.h>

enum { maxn = 7 };

int main() {
    int n, k, count = 0;
    scanf("%d", &n);
    int m[maxn][maxn];
    for (int i = 0; i != n; ++i) {
        for (int j = 0; j != n; ++j) {
            scanf("%d", &m[i][j]);
        }
    }

    for (int i = 0; i != -1; ++i) {
        int k = i;
        while(k >= 0) {
            if (i % 2 == 0) {
                if ((n - 1 - k) >= 0 && (n - 1 - k) < n && (i - k) >= 0 && (i - k) < n) {
                    printf("%d ", m[n - 1 - k][i - k]);
                    count += 1;
                }
            } else {
                if ((n - 1 - (i - k)) >= 0 && (n - 1 - (i - k)) < n && (k) >= 0 && (k) < n) {
                    printf("%d ", m[n - 1 - (i - k)][k]);
                    count += 1;
                }
            }
            k -= 1;
            if (count == (n * n)) {
                break;
            }
        }
        if (count == (n * n)) {
            break;
        }
   }
    printf("\n");  
}
